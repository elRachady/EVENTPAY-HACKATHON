# LNbits JS Example .env file
MINT_LIGHTNING_BACKEND=CoreLightningWallet
# Base URL of your LNbits instance (e.g., http://localhost:5000)
LNBITS_NODE_URL=http://localhost:5000

# Admin API key for LNbits (keep this secret!)
LNBITS_ADMIN_KEY=

# Invoice/read-only API key for LNbits
LNBITS_INVOICE_KEY=

# Optional: Wallet ID to use by default
LNBITS_WALLET_ID=

# Port for the server to run on
PORT=3006

# Node environment (development, production, etc.)
NODE_ENV=development